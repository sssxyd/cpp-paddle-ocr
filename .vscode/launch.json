{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug paddle_ocr",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/paddle_ocr.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}/bin/msvc;${env:PATH}"
        }
      ],
      "console": "integratedTerminal",  
      "preLaunchTask": "build-debug"
    },    {
      "name": "Debug OCR Tests",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/test_ocr_worker.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/tests/build",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}/bin/msvc;${workspaceFolder}/tests/build;${env:PATH}"
        }
      ],
      "console": "integratedTerminal",
      "preLaunchTask": "build-ocr-tests"
    },    
    {
      "name": "Debug Single Test (RealImageProcessing)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/test_ocr_worker.exe",
      "args": ["RealImageProcessing"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/tests/build",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}/bin/msvc;${workspaceFolder}/tests/build;${env:PATH}"
        }
      ],
      "console": "integratedTerminal",
      "preLaunchTask": "build-ocr-tests"
    },
    {
      "name": "Debug Single Test (ColdVsWarmStartup)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/test_ocr_worker.exe",
      "args": ["ColdVsWarmStartup"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/tests/build",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceFolder}/bin/msvc;${workspaceFolder}/tests/build;${env:PATH}"
        }
      ],
      "console": "integratedTerminal",
      "preLaunchTask": "build-ocr-tests"
    }
  ]
}